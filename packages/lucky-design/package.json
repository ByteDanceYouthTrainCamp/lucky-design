{
  "name": "lucky-design",
  "type": "module",
  "version": "1.0.0",
  "description": "A Component Library for Vue 3",
  "license": "MIT",
  "keywords": [
    "lucky-design",
    "components",
    "unocss",
    "vue",
    "vueuse"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/lucky-design.umd.cjs",
      "import": "./dist/lucky-design.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/lucky-design.umd.cjs",
  "module": "./dist/lucky-design.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/lucky-design-org/lucky-design/issues"
  },
  "homepage": "https://lucky-design.vercel.app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucky-design-org/lucky-design.git"
  },
  "scripts": {
    "build": "vue-tsc --declaration --emitDeclarationOnly --skipLibCheck && vite build",
    "dev": "esno ../../scripts/dev.ts"
  },
  "dependencies": {
    "@lucky-design/components": "workspace:*",
    "@lucky-design/preset": "workspace:*",
    "@lucky-design/common": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
